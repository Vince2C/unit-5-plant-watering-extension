{
    "manifest_version": 2,
    "name": "Jess, Kylene, Vince Waterpot game!",
    "version": "3", 
    "content_scripts":[
    {
        "matches": [
            "<all_urls>"
        ],
        "js": ["main.js"]
        }
    ],

    "browser_action":{
        "default_icon": "assets/icon16.png", 
        "default_popup": "index.html", 
        "default_title": "Generates the game" 
    },
    "icons":{ 
        "16": "assets/icon16.png",
        "48": "assets/icon48.png",
       "128": "assets/icon128.png" 
    }
}